[user]
	name = Chris Evans
	email = chris.evans93@gmail.com
[core]
	editor = nvim
	excludesfile = /Users/Chris/.gitignore_global
[color]
	diff = auto
	status = auto
	branch = auto
[alias]
	co = checkout
	cm = commit
	st = status
	br = branch
	lg = log --graph --pretty='format:%C(yellow)%h %Creset%s%Cred%d'
	hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
	type = cat-file -t
	dump = cat-file -p
[push]
	default = simple
